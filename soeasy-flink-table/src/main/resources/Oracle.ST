#预加载事中配置信息
SELECT_ONPROCESS_SQL = "select * from fr_processingrule_info";


#预加载当前有效规则（通过FR_BUSIRULE_INFO的RULE_STATE判断）
SELECT_RULE_SQL = "SELECT BR.*,R.* FROM   FR_BUSIRULE_INFO BR inner JOIN FR_ENG_RULE_INFO R  ON R.RULE_ID = BR.RULE_ID  where  BR.RULE_STATE>0 and BR.RULE_TYPE='2'  and BR.APP_CLASS in('1','3')  AND BR.PUB_STAT='1'  union all SELECT BR.*,R.* FROM   FR_BUSIRULE_INFO BR inner JOIN FR_ENG_RULE_INFO R  ON (R.RULE_ID = BR.RULE_ID  AND R.ORG_ID = BR.ORG_ID) where   BR.RULE_STATE>0 and BR.RULE_TYPE='2'  and BR.APP_CLASS ='2'";


#规则路由SQL
SELECT_ROUTING_SQL = "SELECT T.TRANSACTION_CODE, T.RULE_SET_ID, T.PROCESS_PRIORITY AS PRIORITY_ORDER, T.PROCESS_ORDER, ft.TAB_NAME FROM fr_eng_transaction_routing T INNER JOIN fr_rule_trans_info ft ON t.TRANSACTION_CODE = ft.trans_code ORDER BY PRIORITY_ORDER, PROCESS_ORDER";

